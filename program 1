#include <iostream>
using namespace std;

struct Item {
    int id;
    float cost;
};

int main() {
    Item items[6]; 
    items[0].id = 101;
    items[0].cost = 45.50;

    items[1].id = 102;
    items[1].cost = 60.00;

    items[2].id = 103;
    items[2].cost = 75.25;

    items[3].id = 104;
    items[3].cost = 30.00;
    
    items[4].id = 0;
    items[4].cost = 0.0;

    items[5].id = 0;
    items[5].cost = 0.0;

    cout << "Items with cost greater than 50.00:\n\n";

    
    for (int i = 0; i < 6; i++) {
        if (items[i].cost > 50.00) {
            cout << "Item ID: " << items[i].id << "\n";
            cout << "Cost: " << items[i].cost << "\n\n";
        }
    }

    return 0;
}

