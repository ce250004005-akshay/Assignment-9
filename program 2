#include <iostream>
using namespace std;

struct Color {
    int red;
    int green;
    int blue;
};

int main() {
    
    Color colors[5] = {
        {120, 60, 200},
        {10, 255, 100},
        {50, 150, 75},
        {0, 100, 255},
        {200, 50, 0}
    };

    
    for (int i = 0; i < 3; i++) {
        colors[i].red = 255 - colors[i].red;
        colors[i].green = 255 - colors[i].green;
        colors[i].blue = 255 - colors[i].blue;
    }

    // Display the RGB values for all 5 elements
    cout << "New RGB values after inversion:\n\n";
    for (int i = 0; i < 5; i++) {
        cout << "Color " << i + 1 << " -> ";
        cout << "R: " << colors[i].red << ", ";
        cout << "G: " << colors[i].green << ", ";
        cout << "B: " << colors[i].blue << "\n";
    }

    return 0;
}
